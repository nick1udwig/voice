{
  "name": "voice-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "copy-caller-utils": "cp ../target/ui/caller-utils.ts ui-call/src/ || true",
    "build:ui-call": "npm run copy-caller-utils && cd ui-call && npm install && npm run build",
    "copy": "mkdir -p ../pkg/ui && rm -rf ../pkg/ui/* && cp -r dist/* ../pkg/ui/",
    "copy:ui-call": "mkdir -p ../pkg/ui-call && rm -rf ../pkg/ui-call/* && cp -r ui-call/dist/* ../pkg/ui-call/",
    "build:copy": "npm run build && npm run build:ui-call && npm run copy && npm run copy:ui-call",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hyperware-ai/client-api": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "http-proxy-middleware": "^2.0.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
